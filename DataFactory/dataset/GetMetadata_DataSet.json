{
	"name": "GetMetadata_DataSet",
	"properties": {
		"linkedServiceName": {
			"referenceName": "SCCU_DataFactory_Storage",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"SubFolder": {
				"type": "string",
				"defaultValue": "@item()"
			}
		},
		"annotations": [],
